{
  "name": "brydge",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "test:client": "cypress run --headed --no-exit",
    "test:server": "vitest --config=vitest.config.js",
    "build:client": "vite build --config vite.client.config.js",
    "build:server": "vite build --config vite.server.config.js",
    "build:app": "vite build src --outDir .dist",
    "dev": "vite preview",
    "doc": "typedoc --out docs/typedocs ./src/**.ts"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/node": "^18.14.6",
    "@types/socket.io": "^3.0.2",
    "@vitest/ui": "^0.29.2",
    "cypress": "^12.7.0",
    "mochawesome": "^7.1.3",
    "typedoc": "^0.24.0-beta.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "simple-peer": "^9.11.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "subscribe": "^1.3.0"
  }
}
